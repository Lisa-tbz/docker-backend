    {
      "name": "CreateValidUser",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 201\", function () {\r",
              "    pm.response.to.have.status(201);\r",
              "});\r",
              "\r",
              "pm.test(\"Response is JSON\", () => {\r",
              "    pm.response.to.have.header(\"Content-Type\");\r",
              "    pm.expect(pm.response.headers.get(\"Content-Type\"))\r",
              "        .to.include(\"application/json\");\r",
              "});\r",
              "\r",
              "    const body = pm.response.json();\r",
              "\r",
              "pm.test(\"User has ID\", () => {\r",
              "    pm.expect(body.id).to.exist;\r",
              "});\r",
              "\r",
              "pm.test(\"Email correct\", () => {\r",
              "    pm.expect(body.email).to.eql(\"max.mustermann@example.com\");\r",
              "});\r",
              "\r",
              "pm.test(\"No password in response\", () => {\r",
              "    pm.expect(body.password).to.be.undefined;\r",
