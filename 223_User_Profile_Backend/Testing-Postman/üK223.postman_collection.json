    {
      "name": "SearchAsAdminWithAge",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"Response is paged\", () => {",
              "    pm.expect(pm.response.json()).to.have.property(\"content\");",
              "});",
              "",
              "pm.test(\"All users have age within 15-25 range\", function () {",
              "    const jsonData = pm.response.json();",
              "    const currentYear = new Date().getFullYear();",
              "    ",
              "    jsonData.content.forEach((user, index) => {",
              "        const birthDate = new Date(user.profile.birthDate);",
              "        const age = currentYear - birthDate.getFullYear();",
              "        pm.expect(age, `User ${user.firstName} (index ${index}) age`).to.be.within(15, 25);",
              "    });",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "auth": {
          "type": "bearer",
          "bearer": [
            {
              "key": "token",
              "value": "eyJhbGciOiJIUzM4NCJ9..hyYh6eMTyIvwVVyaH9gBk9NmsAtFtyPBXqHu5yxxK_IHy7mvTSRevU2Z1ceQzQpd",
